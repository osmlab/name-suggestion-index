  "type": "module",
  "exports": {
    "dist/*": "./dist/*",
    "import": "./dist/javascript/nsi.mjs",
    "require": "./dist/javascript/nsi.cjs",
    "browser": "./dist/javascript/nsi.iife.js"
  },
  "scripts": {
    "all": "run-s clean lint build test",
    "appbuild": "bun run docbuild",
    "docbuild": "esbuild ./app/src/index.jsx --platform=browser --target=es2021 --format=iife --global-name=nsiguide --jsx=automatic --bundle --minify --sourcemap --outfile=./docs/index.js",
    "build": "run-s build:js build:features build:index",
    "build:js": "bun ./scripts/build_js.ts",
    "build:features": "bun run ./scripts/build_features.ts",
    "build:index": "bun run ./scripts/build_index.js",
    "clean": "bun run ./scripts/clean.ts",
    "dist": "run-s dist:version dist:files",
    "dist:files": "bun run ./scripts/dist_files.js",
    "dist:version": "bun run ./scripts/dist_version.js",
    "test": "bun test --dots --coverage ./test/*.js",
    "lint": "eslint ./lib/*.js ./test/*.js",
    "validate": "bun run ./scripts/validate.js",
    "wikidata": "bun run ./scripts/build_wikidata.js"
  },
  "dependencies": {
    "diacritics": "^1.3.0",
